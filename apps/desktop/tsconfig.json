{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": "."
  },
  "include": [
    "next-env.d.ts",
    "middleware.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "./out/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "src-tauri"
  ]
}
